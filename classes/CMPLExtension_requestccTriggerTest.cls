@isTest
 public without sharing class CMPLExtension_requestccTriggerTest {
	@IsTest
	private static void testAutoGeneratedTrigger() {
		User u = TestSetup.createUser(true);
		System.runas(u){
			Id ccRecType = Schema.SObjectType.CMPL123QMS__Change_Control__c.getRecordTypeInfosByName().get('CC Custom').getRecordTypeId();
			CMPL123QMS__Change_Control__c cc = TestSetup.createCC(true, ccRecType);
			cc.CMPL123_WF_Status__c = 'Open';
			cc.Change_Owner__c = u.id;
			update cc;
			Extension_Request_Change_Control__c extReq = TestSetup.createExtCC(true, cc.id);
			update extReq;
			delete extReq;
			undelete extReq;
		}
	}
}