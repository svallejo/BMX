            <value>
                <elementReference>PredefinedActionItem.Description__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ActionItem.Change_Control__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>OriginatingChangeControl.Id</elementReference>
            </value>
        </assignmentItems>
