            <assignToReference>ActionItem.CMPL123Rule_Key_Value__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Predefined</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ActionItem.RecordTypeId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Retrieve_Locked_Record_Type_Id.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Add_to_Implementation_Plan</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Action_Items_Found</name>
        <label>Action Items Found?</label>
        <locationX>807</locationX>
        <locationY>158</locationY>
        <defaultConnector>
            <targetReference>NoActionItemsFound</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Action Items Found</defaultConnectorLabel>
        <rules>
            <name>Yes_Action_Items_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ListofPredefinedActionItems</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Retrieve_Locked_Record_Type_Id</targetReference>
            </connector>
            <label>Yes, Action Items Found</label>
        </rules>
    </decisions>
    <decisions>
        <name>Implementation_Plan_Locked</name>
        <label>Implementation Plan Locked?</label>
        <locationX>251</locationX>
        <locationY>158</locationY>
        <defaultConnector>
            <targetReference>SelectPredefinedImplementationPlan</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Is Not Locked</defaultConnectorLabel>
        <rules>
            <name>Is_Locked</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>OriginatingChangeControl.CMPL123_WF_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Open</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ImplementationPlan_Locked</targetReference>
            </connector>
            <label>Is Locked</label>
        </rules>
    </decisions>
    <description>Add in record type assignment for locked page layout for predefined action items when created.</description>
    <dynamicChoiceSets>
        <name>ListofPredefinedImplementationPlans</name>
        <dataType>String</dataType>
        <displayField>Name</displayField>
