            <assignToReference>ActionItem.Predefined_Implementation_Plan__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>PredefinedImplementationName</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ActionItem.Required__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ActionItem.Short_Description__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Short_Description</elementReference>
            </value>
