    <validationRules>
        <fullName>limit_action_items_to_10000</fullName>
        <active>true</active>
        <description>You can not have more than 10,000 Action Item records related to one Change Control</description>
        <errorConditionFormula>ISNEW() &amp;&amp;  Change_Control__r.Action_Item_Records__c = 10000</errorConditionFormula>
        <errorMessage>You can not have more than 10,000 Action Item records related to one Change Control</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCmustbeOpenstatustocancelaction</fullName>
        <active>false</active>
        <description>Parent Change Control must be in Open status to cancel the Action Item</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp;
CMPL123_WF_Action__c =="Cancel" &amp;&amp;
(Change_Control__r.CMPL123_WF_Status__c &lt;&gt; "Open" || Change_Control__r.CMPL123_WF_Status__c &lt;&gt; "Closed - Canceled")</errorConditionFormula>
        <errorMessage>Parent Change Control must be in Open status to cancel the Action Item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quality_Approver_is_required_if_it_requi</fullName>
        <active>true</active>
        <description>If Required Approval is Checked Assignee must populate a Quality Approver in order to move to the next status</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp;
CMPL123_WF_Action__c =="Action Complete" &amp;&amp;
Require_Approval__c == True &amp;&amp;

( Quality_Approver__c == null)</errorConditionFormula>
        <errorDisplayField>Quality_Approver__c</errorDisplayField>
        <errorMessage>If Required Approval is Checked Assignee must populate a Quality Approver in order to move to the next status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CC_is_in_Pending_CRB_lock_child_recordai</fullName>
        <active>true</active>
        <description>All Child Records are locked when the Change Control is in the status of Pending CRB Team Review</description>
        <errorConditionFormula>Change_Control__r.CMPL123_WF_Status__c = 'Pending CRB Team Review'</errorConditionFormula>
        <errorMessage>All Child Records are locked when the Change Control is in the status of Pending CRB Team Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockAI_when_CC_is_Pending_Quality_Revie</fullName>
        <active>true</active>
        <description>All Child Records are locked when the Change Control is in the status of Pending Quality Review</description>
        <errorConditionFormula>Change_Control__r.CMPL123_WF_Status__c = 'Pending Quality Review'</errorConditionFormula>
        <errorMessage>All Child Records are locked when the Change Control is in the status of Pending Quality Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Change_Owner_or_Change_Coordinator</fullName>
        <active>true</active>
        <description>Only Change Owner or Change Coordinator can modify fields in the initiation, impact assessment, and implementation plan sections after CRB Approval and the WF was sent back to Open</description>
        <errorConditionFormula>(!Includes($User.TWD_Role__c, 'Change Coordinator') &amp;&amp;  $User.Id !=  Change_Control__r.Change_Owner__c  ) &amp;&amp;  Change_Control__r.Only_Change_Owner_or_Change_Coordinator__c</errorConditionFormula>
        <errorMessage>Only Change Owner or Change Coordinator can modify fields in the initiation, impact assessment, and implementation plan sections after CRB Approval and the WF was sent back to Open</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Action_Items_after_CC_is_reviewed</fullName>
        <active>true</active>
        <description>Action Items can only be created when Change Control is in the Open status</description>
        <errorConditionFormula>AND (
ISNEW(),
Change_Control__r.CMPL123_WF_Status__c   &lt;&gt;  'Open'
)</errorConditionFormula>
        <errorMessage>Action Items can only be created when Change Control is in the Open status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approver_1_Must_Be_Populated</fullName>
        <active>true</active>
        <description>Approver 1 field must be populated if optional approvals are required.</description>
        <errorConditionFormula>ISBLANK( Approver_1__c) &amp;&amp; NOT(AND(ISBLANK( Approver_2__c ),ISBLANK( Approver_3__c ),ISBLANK( Approver_4__c ),ISBLANK( Approver_5__c ), ISBLANK( Approver_6__c )))</errorConditionFormula>
        <errorDisplayField>Approver_1__c</errorDisplayField>
        <errorMessage>Reassign an approver into the Approver 1 field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Af_Releaseonlyeditable_in_Open_or_arip</fullName>
        <active>true</active>
        <description>After Release Items can only be edited in either Open status or After Items in Progress.</description>
        <errorConditionFormula>After_Release__c = True &amp;&amp;  ((Change_Control__r.CMPL123_WF_Status__c &lt;&gt; "Open") &amp;&amp; (Change_Control__r.CMPL123_WF_Status__c &lt;&gt; "After Release Items in Progress"))</errorConditionFormula>
        <errorMessage>After Release Items can only be edited in either Open status or After Items in Progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Fields_before_CC_is_Approved</fullName>
        <active>true</active>
        <description>Summary of Action Taken is locked until CC is Approved</description>
        <errorConditionFormula>(ISCHANGED( Summary_of_Action_Taken__c ) || (ISNEW() &amp;&amp; !ISBLANK(Summary_of_Action_Taken__c))) 
&amp;&amp;
((Change_Control__r.CMPL123_WF_Status__c = "Open") || 
(Change_Control__r.CMPL123_WF_Status__c = "Pending CRB Team Review") || 
(Change_Control__r.CMPL123_WF_Status__c = "Pending Quality Review"))</errorConditionFormula>
        <errorDisplayField>Summary_of_Action_Taken__c</errorDisplayField>
        <errorMessage>Summary of Action Taken is locked until CC is Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_Fields_before_CC_is_Approved_URL</fullName>
        <active>true</active>
        <description>URL (for PECN use) is locked until CC is Approved</description>
        <errorConditionFormula>(ISCHANGED(  URL_for_PECN_use__c  ) || (ISNEW() &amp;&amp; !ISBLANK(URL_for_PECN_use__c ))) 
&amp;&amp;
((Change_Control__r.CMPL123_WF_Status__c = "Open") || 
(Change_Control__r.CMPL123_WF_Status__c = "Pending CRB Team Review") || 
(Change_Control__r.CMPL123_WF_Status__c = "Pending Quality Review"))</errorConditionFormula>
        <errorDisplayField>URL_for_PECN_use__c</errorDisplayField>
        <errorMessage>URL (for PECN use) is locked until CC is Approved</errorMessage>
    </validationRules>
    <validationRules>
