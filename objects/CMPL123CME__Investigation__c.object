    <validationRules>
        <fullName>Additionnal_info_required_for_reclosure</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK(BMX_First_Investigation_Closed_Date__c)),ISBLANK( BMX_Additional_Information__c ), CMPL123_WF_Action__c ="Complete Investigation")</errorConditionFormula>
        <errorMessage>Additionnal Information is mandatory to close an Investigation previously reopened.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annex_BCD_Code_choice_should_not_B21_C21</fullName>
        <active>false</active>
        <description>If Annex BCD Contains the value "B21 , C21 ,D16 then Investigation cannot be closed</description>
        <errorConditionFormula>OR(

AND(
CMPL123_WF_Status__c = 'Complete Investigation', 
OR(ISPICKVAL(B_Investigation_Type_Code_Choice_1__c, 'B21 - Type of Investigation Not Yet Determined'),
ISPICKVAL(B_Investigation_Type_Code_Choice_2__c    , 'B21 - Type of Investigation Not Yet Determined'),
ISPICKVAL(B_Investigation_Type_Code_Choice_3__c    , 'B21 - Type of Investigation Not Yet Determined'))
),

AND(
CMPL123_WF_Action__c = 'Complete Investigation',
OR(
C_Investigation_Findings_Code_Choice1__r.Name &lt;&gt; "C21",
C_Investigation_Findings_Code_Choice2__r.Name &lt;&gt; "C21",
C_Investigation_Findings_Code_Choice3__r.Name &lt;&gt; "C21"),

AND(
CMPL123_WF_Action__c = 'Complete Investigation',
OR(
D_Investigation_Conclusion_Choice1__r.Name &lt;&gt; "D21",
