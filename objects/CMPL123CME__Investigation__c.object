    <validationRules>
        <fullName>BMX_CAPA_Justification_should_not_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( BMX_CAPA_Required__c, 'No'),ISBLANK(  BMX_CAPA_Justification__c ) )</errorConditionFormula>
        <errorDisplayField>BMX_CAPA_Justification__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_CAPA_Number_should_not_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( BMX_CAPA_Required__c, 'Yes'),ISBLANK(   BMX_CAPA_Number__c ) )</errorConditionFormula>
        <errorDisplayField>BMX_CAPA_Number__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Correction_Number_Required</fullName>
        <active>true</active>
        <description>Correction number field is mandatory when Correction Required Field is set YES</description>
        <errorConditionFormula>AND(ISPICKVAL(BMX_Correction_Required__c, 'Yes'),
ISBLANK( Correction_Number__c ))</errorConditionFormula>
        <errorDisplayField>Correction_Number__c</errorDisplayField>
        <errorMessage>Correction Number field is mandatory.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Date_Product_Recvd_should_nt_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(BMX_Product_Returned__c, 'Product Return Received'),   ISBLANK( BMX_Date_Product_Received__c ) )</errorConditionFormula>
        <errorDisplayField>BMX_Date_Product_Received__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Evidence_of_Trend_should_nt_be_blank</fullName>
        <active>true</active>
        <description>US-INV-029 change log</description>
        <errorConditionFormula>AND( ISPICKVAL(BMX_Is_there_a_trend__c, 'Yes'),   ISBLANK(BMX_Evidence_of_Trend__c) )</errorConditionFormula>
        <errorDisplayField>BMX_Evidence_of_Trend__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Fab_Justification_should_nt_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( BMX_FAB_Required__c , 'No'),   ISBLANK(  BMX_FAB_Justification__c ) )</errorConditionFormula>
        <errorDisplayField>BMX_FAB_Justification__c</errorDisplayField>
        <errorMessage>You must enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Locked_record_when_status_is_Opened</fullName>
        <active>false</active>
        <description>User can edit Additional Information field when record is Reopened</description>
        <errorConditionFormula>AND(  CMPL123_WF_Action__c = 'Reopen',  
      NOT(ISCHANGED(BMX_Additional_Information__c)),
      ISCHANGED( BMX_Comments_Available__c )
      )</errorConditionFormula>
        <errorMessage>Record Locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Patient_Data_Destroy_not_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(BMX_Patient_Data_Copied_to_BMX_System__c,'Yes'),
ISBLANK(TEXT(BMX_All_Copies_of_Patient_Data_Destroyed__c)))</errorConditionFormula>
        <errorDisplayField>BMX_All_Copies_of_Patient_Data_Destroyed__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Prevent_Edit_Investigation</fullName>
        <active>true</active>
        <errorConditionFormula>CMPL123CME__Complaint__r.CMPL123CME__CMPL123_WF_Status__c == "Closed - Completed"</errorConditionFormula>
        <errorMessage>Record Locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Prevent_Inv_Frm_Create_or_Edit</fullName>
        <active>true</active>
        <description>Prevent Editing or adding investigation when the complaint is closed</description>
        <errorConditionFormula>OR( 
AND(ISNEW(), CMPL123CME__Complaint__r.CMPL123CME__CMPL123_WF_Status__c ='Closed'), 
CMPL123CME__Complaint__r.CMPL123CME__CMPL123_WF_Status__c ='Closed')</errorConditionFormula>
        <errorMessage>Can not create/edit a Investigation for closed Complaint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_Prevent_edit_for_closed_inquiry</fullName>
        <active>true</active>
        <errorConditionFormula>Inquiry__r.CMPL123_WF_Status__c =="Closed - Complaint"</errorConditionFormula>
        <errorMessage>Record Locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BMX_RCC_should_not_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CMPL123_WF_Action__c = 'Complete Investigation', ISBLANK(     Root_Cause_Code__c ) )</errorConditionFormula>
        <errorDisplayField>Root_Cause_Code__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Investigation_Steps_are_Completed</fullName>
        <active>true</active>
        <description>All fields in all investigation steps need to be completed prior to performing "Complete Investigation" on the record.</description>
        <errorConditionFormula>AND(CMPL123_WF_Action__c = 'Complete Investigation', BMX_Number_of_Questions_Unanswered__c &gt; 0)</errorConditionFormula>
        <errorMessage>Complete Investigation Steps</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expert_Investigator_should_not_be_blank</fullName>
        <active>true</active>
        <description>For investigation, system to check that at least one of the expert investigator fields is populated prior to the record moving to "Pending Expert Investigation" status.</description>
        <errorConditionFormula>AND(
OR(CMPL123_WF_Action__c = 'Require Expert Investigation', CMPL123_WF_Action__c = 'Rework'),
AND(ISBLANK( BMX_Expert_Investigator__c),
ISBLANK(BMX_Expert_Investigator_text__c)),
ISCHANGED(CMPL123_WF_Action__c)
)</errorConditionFormula>
        <errorMessage>One of the Expert investigator is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Investigation_Approver_Should_nt_b_blnk</fullName>
        <active>true</active>
        <description>Investigation Approver field should be populated prior to entering "Pending Approval" status.</description>
        <errorConditionFormula>AND(CMPL123_WF_Action__c = 'Complete Investigation', 
    ISBLANK(BMX_Investigation_Approver__c))</errorConditionFormula>
        <errorMessage>Investigation Approver must not be blank</errorMessage>
    </validationRules>
