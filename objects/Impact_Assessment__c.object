    <validationRules>
        <fullName>Other_SME_Area</fullName>
        <active>true</active>
        <description>If Other is selected in SME Area, then Other - SME Area must be populated.</description>
        <errorConditionFormula>AND(
ISPICKVAL(SME_Area__c, "Other"),
ISBLANK( Other_SME_Area__c )
)</errorConditionFormula>
        <errorDisplayField>Other_SME_Area__c</errorDisplayField>
        <errorMessage>If Other is selected in SME Area, then Other - SME Area must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QA_assessment_if_created_must_be_last</fullName>
        <active>true</active>
        <description>Quality Assurance assessment (if created) must be completed last</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp; 
CMPL123_WF_Action__c =="Assessment Completed" &amp;&amp; 
(ISPICKVAL(SME_Area__c, "Quality Assurance") &amp;&amp;

( Change_Control__r.Closed_Impact_Assessments__c   &lt;&gt;   Change_Control__r.Total_Impact_Assessments_with_type_quali__c ))</errorConditionFormula>
        <errorMessage>Quality Assurance assessment must be completed last</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Assignee_restrictions</fullName>
        <active>true</active>
        <description>Only the Change Owner of the Change Control or Change Coordinator can modify the Assignee field</description>
        <errorConditionFormula>(PRIORVALUE(Assignee__c) != null &amp;&amp; Change_Control__r.Change_Owner__c  &lt;&gt;  $User.Id &amp;&amp; ISCHANGED( Assignee__c ))
&amp;&amp;
( PRIORVALUE(Assignee__c) != null &amp;&amp; PRIORVALUE(Assignee__c) &lt;&gt; $User.Id &amp;&amp; ISCHANGED( Assignee__c ))
&amp;&amp;
( PRIORVALUE(Assignee__c) != null &amp;&amp;   (!Includes($User.TWD_Role__c, 'Change Coordinator') &amp;&amp; ISCHANGED( Assignee__c )))</errorConditionFormula>
        <errorMessage>Only the Assignee, Change Owner of the Change Control, or Change Coordinator can modify the Assignee field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CC_is_in_Pending_CRB_lock_child_records</fullName>
        <active>true</active>
        <description>All Child Records are locked when the Change Control is in the status of Pending CRB Team Review</description>
        <errorConditionFormula>Change_Control__r.CMPL123_WF_Status__c  = 'Pending CRB Team Review'</errorConditionFormula>
        <errorMessage>All Child Records are locked when the Change Control is in the status of Pending CRB Team Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_when_CC_is_in_Pending_Quality_Revie</fullName>
        <active>true</active>
        <description>All Child Records are locked when the Change Control is in the status of Pending Quality Review</description>
        <errorConditionFormula>Change_Control__r.CMPL123_WF_Status__c == "Pending Quality Review" &amp;&amp; NOT(Performed_By_Automation__c)</errorConditionFormula>
        <errorMessage>All Child Records are locked when the Change Control is in the status of Pending Quality Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Change_Owner_or_Change_Coordinator</fullName>
        <active>false</active>
        <description>Only Change Owner or Change Coordinator can modify fields in the initiation, impact assessment, and implementation plan sections after CRB Approval and the WF was sent back to Open</description>
        <errorConditionFormula>(!Includes($User.TWD_Role__c, 'Change Coordinator') &amp;&amp;  $User.Id !=  Change_Control__r.Change_Owner__c  ) &amp;&amp;  Change_Control__r.Only_Change_Owner_or_Change_Coordinator__c</errorConditionFormula>
        <errorMessage>Only Change Owner or Change Coordinator can modify fields in the initiation, impact assessment, and implementation plan sections after CRB Approval and the WF was sent back to Open</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impact_Assessments_only_in_cc_open</fullName>
        <active>true</active>
        <description>Impact Assessments can only be created when Change Control is in the Open status</description>
        <errorConditionFormula>AND (
ISNEW(),
Change_Control__r.CMPL123_WF_Status__c   &lt;&gt;  'Open'
)</errorConditionFormula>
        <errorMessage>Impact Assessments can only be created when Change Control is in the Open status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_more_impact_assessments_after_quality</fullName>
        <active>true</active>
        <description>You cannot add an addtional Impact Assessment after the Quality Assurance Assessment was completed</description>
        <errorConditionFormula>AND (
ISNEW(),
 Change_Control__r.Total_Quality_Assessments_done__c  &gt; 0
)</errorConditionFormula>
        <errorMessage>You cannot add an addtional Impact Assessment after the Quality Assurance Assessment was completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Impact_Assessme_after_CC_is_reviewed</fullName>
        <active>true</active>
        <description>Impact Assessments can only be created when Change Control is in the Open status</description>
        <errorConditionFormula>AND (
ISNEW(),
Change_Control__r.CMPL123_WF_Status__c &lt;&gt; 'Open'
)</errorConditionFormula>
        <errorMessage>Impact Assessments can only be created when Change Control is in the Open status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Impact_Assessment_Restriction</fullName>
        <active>true</active>
        <description>Only the Assignee can modify the Impact Assessment field</description>
        <errorConditionFormula>Assignee__c &lt;&gt; $User.Id &amp;&amp; 
( ISCHANGED( impact_Assessment__c ) ||
(ISNEW() &amp;&amp; !ISBlANK( impact_Assessment__c )))</errorConditionFormula>
        <errorDisplayField>impact_Assessment__c</errorDisplayField>
        <errorMessage>Only the Assignee can modify the Impact Assessment field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Implementation_Action_Restriction</fullName>
        <active>true</active>
        <description>Only the Assignee can modify the Implementation Action field</description>
        <errorConditionFormula>Assignee__c &lt;&gt; $User.Id &amp;&amp; 
( ISCHANGED(Implementation_Action__c) ||
(ISNEW() &amp;&amp; !ISBlANK(Implementation_Action__c)))</errorConditionFormula>
        <errorDisplayField>Implementation_Action__c</errorDisplayField>
        <errorMessage>Only the Assignee can modify the Implementation Action field</errorMessage>
    </validationRules>
