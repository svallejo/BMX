    <validationRules>
        <fullName>Action_Item_created_before_Revie</fullName>
        <active>true</active>
        <description>At least one Action Item must be created prior to performing "Submit for Review".</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp; 
CMPL123_WF_Action__c =="Submit for Review" &amp;&amp; 
 
( Total_Action_Items__c   = 0)</errorConditionFormula>
        <errorMessage>At least one Action Item must be created prior to performing "Submit for Review".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Action_Items_Must_be_Closed</fullName>
        <active>true</active>
        <description>All action items must be in a closed status before user can perform "Implementation Complete" or "After Release Items Complete"</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp; 
(CMPL123_WF_Action__c == "Implementation Complete" || CMPL123_WF_Action__c == "After Release Items Complete") &amp;&amp; 
(  Total_Actions_Open__c    &lt;&gt;  0 )</errorConditionFormula>
        <errorMessage>All action items must be in a closed status before user can perform "Implementation Complete" or "After Release Items Complete"</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Impact_Ass_Closed_before_Review</fullName>
        <active>true</active>
        <description>All impact assessments created must be completed prior to performing "Submit for Review</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp; 
CMPL123_WF_Action__c =="Submit for Review" &amp;&amp; 

(Total_Impact_Assessments_Completed__c  &lt;&gt;  Total_impact_Assessments__c )</errorConditionFormula>
        <errorMessage>All impact assessments created must be completed prior to performing Submit for Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_PreRelease_Action_Items_are_Closed</fullName>
        <active>true</active>
        <description>"Submit for Release Approval" can only be performed when all action items that do not have After Release checked are in a closed status</description>
        <errorConditionFormula>ISCHANGED( CMPL123_WF_Action__c ) &amp;&amp; 
CMPL123_WF_Action__c == "Submit for Release Approval" &amp;&amp; 

( After_Release_Action_Items_Not_Closed__c   &lt;&gt;  0 )</errorConditionFormula>
        <errorMessage>"Submit for Release Approval" can only be performed when all action items that do not have After Release checked are in a closed status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRBCommentsonly_changed_at_crb_or_qa_rev</fullName>
        <active>true</active>
        <description>CRB Comments can only be changed at CRB Team Review or Quality Review</description>
        <errorConditionFormula>(NOT(CMPL123_WF_Status__c == "Pending CRB Team Review" || CMPL123_WF_Status__c =="Pending Quality Review") &amp;&amp; ISCHANGED( CRB_Comments__c )) || (ISNEW() &amp;&amp; NOT(ISBLANK(CRB_Comments__c)))</errorConditionFormula>
        <errorMessage>CRB Comments can only be changed at CRB Team Review or Quality Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRB_Approver_1_Must_Be_Populated</fullName>
        <active>true</active>
        <description>CRB Approver 1 field must be populated if optional approvals are required.</description>
        <errorConditionFormula>ISBLANK( CRB_Approver_1__c ) &amp;&amp; NOT(AND(ISBLANK( CRB_Approver_2__c ),ISBLANK( CRB_Approver_3__c ),ISBLANK( CRB_Approver_4__c ),ISBLANK( CRB_Approver_5__c ), ISBLANK( CRB_Approver_6__c )))</errorConditionFormula>
        <errorDisplayField>CRB_Approver_1__c</errorDisplayField>
        <errorMessage>Reassign an approver into the CRB Approver 1 field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Imp_Quest_Required_for_non_admin</fullName>
