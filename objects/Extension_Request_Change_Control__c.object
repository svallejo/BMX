    <validationRules>
        <fullName>Extension_Request_Cannot_Be_Approved</fullName>
        <active>true</active>
        <description>Extension Request Cannot Be Approved When Change Control is Closed.</description>
        <errorConditionFormula>CONTAINS( Change_Control__r.CMPL123_WF_Status__c, "Closed") &amp;&amp;  CMPL123_WF_Action__c = "Approve"</errorConditionFormula>
        <errorMessage>Extension request cannot be approved for a closed change control.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proposed_Due_Date_cannot_be_in_the_past</fullName>
        <active>true</active>
        <errorConditionFormula>Proposed_Due_Date__c &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Proposed_Due_Date__c</errorDisplayField>
        <errorMessage>Proposed Due Date cannot be in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Change_Owner_or_Change_Coordinator</fullName>
        <active>false</active>
        <description>Only Change Owner or Change Coordinator can modify fields in the initiation, impact assessment, and implementation plan sections after CRB Approval and the WF was sent back to Open</description>
        <errorConditionFormula>(!Includes($User.TWD_Role__c, 'Change Coordinator') &amp;&amp;  $User.Id !=  Change_Control__r.Change_Owner__c  ) &amp;&amp;  Change_Control__r.Only_Change_Owner_or_Change_Coordinator__c</errorConditionFormula>
        <errorMessage>Only Change Owner or Change Coordinator can modify fields in the initiation, impact assessment, and implementation plan sections after CRB Approval and the WF was sent back to Open</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Extension_Requester_restrictions</fullName>
        <active>true</active>
        <description>Only Change Coordinator can update the extension requester field to assign to a different user.</description>
        <errorConditionFormula>( PRIORVALUE( Extension_Requester__c ) != null &amp;&amp;   (!Includes($User.TWD_Role__c, 'Change Coordinator') &amp;&amp; ISCHANGED( Extension_Requester__c  )))</errorConditionFormula>
        <errorMessage>Only Change Coordinator can update the extension requester field to assign to a different user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>limit_who_can_open_extension_request</fullName>
        <active>true</active>
        <description>Only Change Owner of the change control or a Change Coordinator can create a new extension request for that record.</description>
        <errorConditionFormula>(!Includes($User.TWD_Role__c, 'Change Coordinator') &amp;&amp;  $User.Id !=  Change_Control__r.Change_Owner__c  ) &amp;&amp; ISNEW()</errorConditionFormula>
        <errorMessage>Only Change Owner of the change control or a Change Coordinator can create a new extension request for that record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_Open_a_Extension_Request_when_clos</fullName>
        <active>true</active>
        <description>You cannot create a extension request on a closed Change Control</description>
        <errorConditionFormula>AND (
ISNEW(),
CONTAINS(Change_Control__r.CMPL123_WF_Status__c, "Closed")
)</errorConditionFormula>
        <errorMessage>You cannot create a extension request on a closed Change Control</errorMessage>
    </validationRules>
